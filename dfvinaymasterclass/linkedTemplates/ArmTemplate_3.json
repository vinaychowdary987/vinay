					],
					"scriptLines": [
						"source(output(",
						"          customerid as integer,",
						"          Name as string,",
						"          {salary } as integer,",
						"          {city } as string,",
						"          phonenumber as long",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> source1",
						"source(output(",
						"          customerid as integer,",
						"          Name as string,",
						"          salary as integer,",
						"          city as string,",
						"          phonenumber as long,",
						"          hashkey as binary,",
						"          isactive as integer",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> source2",
						"source1 derive(SRC_HASH = toBinary(md5(phonenumber)),",
						"          ACTIVE = 1) ~> derivedColumn1",
						"source2 derive(SINK_HASH = toBinary(md5(phonenumber)),",
						"          ACTIVE1 = 0) ~> derivedColumn2",
						"derivedColumn1, derivedColumn2 exists(hex(SRC_HASH) == hex(SINK_HASH),",
						"     negate:true,",
